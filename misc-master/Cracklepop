public class Cracklepop {
    public static void main(String[] args) {
 
        for (int i = 1; i <= 100; i++) {
            String printString = "";
            printString += (i % 3) == 0 ? "Crackle" : "";
            printString += (i % 5) == 0 ? "Pop" : "";
            System.out.println(printString.isEmpty() ? i : printString);
        }
    }
}
